exec 3>/dev/null


#--debug 1 --ip 1.1.1.1 

deps=(flex gperf libcap-devel libcurl-devel libgmp-devel \
			libldap-devel libpam-devel libssl-devel libxml2-devel)

	for item in ${deps[*]}
	do
		if [[ $(rpm -qa $item) != '' ]]; then
			echo "$item already exist"
		else
			echo "$item installing..."
			apt-get install -y $item >&3
		fi
	done